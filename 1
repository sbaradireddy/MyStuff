SELECT 
    A.POL_PK,
    A.DMV_TRANS_TYPE,
    A.REINST_TYP,
    A.ROW_STAT,
    COALESCE(B.POL_PK, 0) AS LKP_REINSTATE_POL_PK, -- From LKP_REINSTATE_WITH_LAPSE1_last
    COALESCE(C.POL_PK, 0) AS LKP_ADD_VEH_POL_PK,  -- From LKP_ADD_VEH_DUPLICATES
    CASE
        WHEN B.POL_PK IS NOT NULL THEN 'Matched in LKP_REINSTATE'
        ELSE 'Not Matched in LKP_REINSTATE'
    END AS REINSTATE_MATCH_STATUS,
    CASE
        WHEN C.POL_PK IS NOT NULL THEN 'Matched in LKP_ADD_VEH'
        ELSE 'Not Matched in LKP_ADD_VEH'
    END AS ADD_VEH_MATCH_STATUS
FROM EXP_PRE_FILTER A
LEFT JOIN LKP_REINSTATE_WITH_LAPSE1_last B
    ON A.POL_PK = B.POL_PK
LEFT JOIN LKP_ADD_VEH_DUPLICATES C
    ON A.POL_PK = C.POL_PK
WHERE A.POL_PK IN (
    SELECT POL_PK
    FROM EXPTRANS1
    WHERE POL_PK NOT IN (SELECT POL_PK FROM SOURCE_TABLE)
);